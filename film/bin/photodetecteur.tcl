#!/bin/sh
# the next line restarts using wish \
exec wish "$0" ${1+"$@"}

set f [info script] 

while {[file type $f] == "link"} {
    set f [file readlink $f]
}

set dir [file dirname $f]
cd $dir

set FIRST 1
set LAST 10

canvas .c -width 800 -height 600
pack .c

frame .fb
pack .fb

for {set i $FIRST} {$i <= $LAST} {incr i} {
    image create photo po$i -file Image_$i.ppm
    image create photo p$i -width 700 -height 600
    if {$i == 1} {
        p$i copy po$i -zoom 2 2 -to 0 2
    } else {
        p$i copy po$i -zoom 2 2
    }
    image create photo b$i -width 70 -height 60
    b$i copy po$i -subsample 5 5
    .c create image 350 300 -image p$i -state hidden -tag p$i
    button .fb.b$i -image b$i
    pack .fb.b$i -side left
    bind .fb.b$i <Enter> "montre $i"
}

bind . <Key-Right> {montreNext 1}
bind . <Key-Up>    {montreNext 1}
bind . <Key-Left>  {montreNext -1}
bind . <Key-Down>  {montreNext -1}

proc montre {i} {
    global P
    if {[info exists P]} {
        .c itemconfigure $P -state hidden
    }
    set P $i
    .c itemconfigure trait -state hidden
    .c itemconfigure $P -state normal
    .c itemconfigure p$P -state normal
}


proc montreNext {i} {
    global P FIRST LAST
    if {![info exists P]} {
        set p 1
    } else {
        set p [expr {$P + $i}]
    }
    if {$p < $FIRST} {
        set p $FIRST
    }
    if {$p > $LAST} {
        set p $LAST
    }
    montre $p
}

