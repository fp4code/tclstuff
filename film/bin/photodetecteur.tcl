proc liminf {list val} {
    set e {}
    foreach l $list {
        if {$l <= $val} {
            if {$e == {} || $e < $l} {
                set e $l
            }
        }
    }
    return $e
}

proc limsup {list val} {
    set e {}
    foreach l $list {
        if {$l >= $val} {
            if {$e == {} || $e > $l} {
                set e $l
            }
        }
    }
    return $e
}

proc interpole {arrayName val} {
    upvar $arrayName array
    set x1 [liminf [array names array] $val]
    set x2 [limsup [array names array] $val]
    if {$x1 == $val || $x2 == $val} {
        return $array($val)
    }
    if {$x1 == {} || $x2 == {}} {
        return -code error "Pas dans un intervalle : $val"
    }
    set y1 $array($x1)
    set y2 $array($x2)
    set y [expr {$y1*double($val-$x2)/double($x1-$x2) + $y2*double($val-$x1)/double($x2-$x1)}]
}

array set reseau $data
array set bragg $dataBragg

proc trait {x tag} {
    global reseau bragg X0 X1 Y0 Y1
    set ya [interpole reseau $x]
    set yb [interpole bragg $x]
    set X [xToX $x 0.7 0.9 $X0 $X1]
    set Ya [xToX $ya 0 1 $Y0 $Y1]
    set Yb [xToX $yb 0 1 $Y0 $Y1]
    if {$Ya > $Yb} {
        set Yu $Yb
        set Yd $Ya
    } else {
        set Yu $Ya
        set Yd $Yb
    }
    set taglist [concat $tag trait]
    .c create line $X 30 $X [expr {$Yd + 10}] -fill green -tag $taglist
    .c create oval [expr {$X - 3}] [expr {$Ya - 3}] [expr {$X + 3}] [expr {$Ya + 3}] -outline cyan -tag $taglist
    .c create oval [expr {$X - 3}] [expr {$Yb - 3}] [expr {$X + 3}] [expr {$Yb + 3}] -outline white -tag $taglist
    .c create text $X 30 -anchor sw -text "[expr {int($x*1000)}] nm" -fill green -tag $taglist -font {Helvetica -16}
}

foreach {i x} {1 0.787 2 0.792 3 0.797 4 0.802 5 0.807 6 0.812 7 0.817 8 0.822 9 0.827 10 0.832} {
    trait $x p$i
}

