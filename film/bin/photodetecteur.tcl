proc xToX {x x1 x2 X1 X2} { 
    return [expr {$X1 + ($x - $x1)*(double($X2 - $X1)/double($x2 - $x1))}]
}

set X0  50
set X1 250
set Y0 210
set Y1  10

catch {unset Xo Yo}
foreach {x y} $data {
    set X [xToX $x 0.7 0.9 $X0 $X1]
    set Y [xToX $y 0   1   $Y0 $Y1]
    if {[info exists Xo] && [info exists Yo]} {
        .c create line $Xo $Yo $X $Y -fill cyan -tag data
    }
    set Xo $X
    set Yo $Y
